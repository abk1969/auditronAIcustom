# API Configuration
REACT_APP_API_URL=http://localhost:8000/api

# Authentication
REACT_APP_GOOGLE_CLIENT_ID=your_google_client_id
REACT_APP_GITHUB_CLIENT_ID=your_github_client_id

# Application
REACT_APP_NAME=AuditronAI
REACT_APP_VERSION=1.0.0
REACT_APP_DESCRIPTION="Plateforme d'audit et d'analyse de code intelligente"

# Features
REACT_APP_ENABLE_SOCIAL_LOGIN=true
REACT_APP_ENABLE_EMAIL_VERIFICATION=true
REACT_APP_ENABLE_TWO_FACTOR_AUTH=false
REACT_APP_ENABLE_DARK_MODE=true
REACT_APP_ENABLE_NOTIFICATIONS=true

# Monitoring
REACT_APP_SENTRY_DSN=your_sentry_dsn
REACT_APP_ENABLE_ERROR_REPORTING=true
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_PERFORMANCE_MONITORING=true

# Misc
REACT_APP_DEFAULT_LANGUAGE=fr
REACT_APP_SUPPORTED_LANGUAGES=fr,en
NODE_ENV=development 